mod DateStruct {
    type alias Year = Int32
    type alias Month = Int32
    type alias Day = Int32


    pub enum Date(Year, Month, Day)

    instance Eq[Date] {
        pub def eq(x: Date, y: Date): Bool = match (x, y) {
                case (Date(y1,m1,d1), Date(y2,m2,d2)) if (y1 == y2 && m1 == m2 && d1 == d2) => true 
                case _ => false
            }
    }
}